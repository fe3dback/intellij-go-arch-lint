<idea-plugin>
    <extensions defaultExtensionNs="JavaScript">
        <JsonSchema.ProviderFactory
                implementation="com.github.fe3dback.intellijgoarchlint.config.schema.GoArchJsonSchemaProviderFactory"
        />
    </extensions>

    <extensions defaultExtensionNs="com.intellij.json">
        <jsonSchemaEnabler
                implementation="com.github.fe3dback.intellijgoarchlint.config.schema.GoArchJsonSchemaEnabler"
        />
    </extensions>

    <extensions defaultExtensionNs="com.intellij.json">
        <jsonSchemaGotoDeclarationSuppressor
                implementation="com.github.fe3dback.intellijgoarchlint.config.schema.GoArchJsonSchemaGotoSuppressor"
        />
    </extensions>
</idea-plugin>