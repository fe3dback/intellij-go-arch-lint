<idea-plugin>
    <id>com.github.fe3dback.intellijgoarchlint</id>
    <name>intellij-go-arch-lint</name>
    <version>0.1</version>
    <vendor>fe3dback</vendor>

    <depends>com.intellij.modules.platform</depends>
    <depends>org.jetbrains.plugins.yaml</depends>

    <extensions defaultExtensionNs="com.intellij">
        <fileType
                name="YAML_GO_ARCH"
                language="yaml"
                implementationClass="com.github.fe3dback.intellijgoarchlint.file.GoArchFileType"
        />
        <completion.contributor
                language="yaml"
                implementationClass="com.github.fe3dback.intellijgoarchlint.GoArchYamlCompletionContributor"
        />

    </extensions>
</idea-plugin>
